#include<iostream>
using namespace std;
const int mub=3;
const int ent=3;
int main()
{
  int s[mub][ent];
cout<<"accept the elements of square matrix "<<endl;
for(int i=0;i<mub;i++){
for(int j=0;j<ent;j++){
cout<<"accept elements of s["<<i<<"] ["<<j<<"]:";
cin>>s[i][j];
}
}
cout<<"square matrix of M\n";
for (int i=0;i<mub;i++){
for (int j=0;j<ent;j++){
cout<<s[i][j]<<"\t";
}
cout<<endl;
}
    return 0;
}
