#include<iostream>
using namespace std;
int main(){
	int a,b,c;
cout <<"Enter three numbers :"<<endl;
cin>>a>>b>>c;
	if(a>b&&a>c){
		cout<<"the greatest number is="<<a<<endl;
	}
	if(b>a&&b>c){
cout<<"the greatest number is="<<b<<endl;
}
if(c>a&&c>b){
cout<<"the greatest number is="<<c<<endl;
}
return 0;
}
    
