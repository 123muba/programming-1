#include<iostream>
using namespace std;
int main(){
	int mark;
cout<<"Enter the mark:"<<endl;
cin>>mark;
	if(mark>=95){
		cout<<"A+ "<<endl;
}
 else if(mark>=85)
{
		 	cout<<"A"<<endl;
	}
  else if(mark>=80){
		cout<<"A-"<<endl;
}
else if(mark>=75)
{
		 	cout<<"B+"<<endl;
	}
else if(mark>=70)
{
		 	cout<<"B"<<endl;
	}
else if(mark>=65)
{
		 	cout<<"B-"<<endl;
	}
 else if(mark>=60)
{
		 	cout<<"C+"<<endl;
	}
 else if(mark>=50)
{
		 	cout<<"C"<<endl;
	}
 else if(mark<50)
{
		 	cout<<"f"<<endl;
	}
return 0;
}
