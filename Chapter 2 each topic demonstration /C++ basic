//c++basic
// c++ single line  comments
/* c++ multi line comments*/ 
#include<iostream>//header file
using namespace std;
#define z 5//c++ constants
int main() //main function
{
 int x;// c++ identifiers
  int y=5;// variable declaration
 cin>>x; //input statements
 cout<<x<<endl<<y<<endl;//output statements
 return 0;// return is  one type of c++ keywords     
}
